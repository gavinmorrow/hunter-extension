{
  "manifest_version": 2,
  "name": "extension-test",
  "version": "0.1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["common.js"]
    },
    {
      "matches": ["https://hunterschools.myschoolapp.com/lms-assignment/*"],
      "js": ["common.js", "fix-month-wrap.js"]
    },
    {
      "matches": [
        "https://hunterschools.myschoolapp.com/app*",
        "https://accounts.google.com/o/oauth2/*",
        "https://accounts.google.com/v3/signin/challenge/pwd?*"
      ],
      "js": ["common.js", "login-automatically.js"]
    }
  ]
}
